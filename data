function assign() {
    a = 2*2;
    println(a);
    a = 5;

    if(a){
        println(a);
    }else{
        println(0);
    };

        a = 0;

        if(a){
            println(a);
        }else{
            println(0);
        };
}

function plus() {
    println(3+3);
}

function minus() {
    println(6-100);
}

function main() {
    println(123);
    assign();
    plus();
    minus();
    println(456);
}